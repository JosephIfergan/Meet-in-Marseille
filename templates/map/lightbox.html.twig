{% block body %}
	<section class="lightbox-creerMeeting">
		<div class="modal fade" id="creerMeetingModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
			<div class="modal-dialog" role="document">
				<div class="modal-content">
					<div class="modal-header">
						<h5 class="modal-title" id="meetingModalLabel">Cr√©er un Meeting</h5>
						<button type="button" class="close" data-dismiss="modal" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<div class="modal-body">
					{{ form_start(form) }}
    {{ form_widget(form) }}
    <button class="btn" id="valider">{{ button_label|default('Valider') }}</button>
{{ form_end(form) }}
					</div>
				</div>
			</div>
		</div>
	</section>
{% endblock %}
